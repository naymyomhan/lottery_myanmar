{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "anaseqal/nova-sidebar-icons": "^0.0.3",
        "coroowicaksono/chart-js-integration": "^0.4.0",
        "cybercog/laravel-ban": "^4.8",
        "cybercog/laravel-nova-ban": "^2.0",
        "devpartners/auditable-log": "^2.0",
        "digital-creative/nova-filepond": "^0.2.13",
        "ebess/advanced-nova-media-library": "^4.0",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.5",
        "jangvel/nova-gutenberg": "^1.0",
        "laraning/nova-time-field": "^1.2",
        "laravel/framework": "^9.19",
        "laravel/nova": "*",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "laravel/vonage-notification-channel": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "maatwebsite/laravel-nova-excel": "^1.3",
        "magdicom/nova-visible-password": "^1.0",
        "michielfb/laravel-nova-time-field": "^1.3",
        "oneduo/nova-time-field": "^1.0",
        "opanegro/field-nova-password-show-hide": "^1.0",
        "outl1ne/nova-simple-repeatable": "^2.1",
        "owen-it/laravel-auditing": "^13.5",
        "php-junior/nova-logs": "^1.1",
        "predis/predis": "^2.0",
        "sereny/nova-permissions": "^1.3",
        "stepanenko3/nova-cards": "^1.1",
        "vonage/client": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "files": [
            "app/Support/Collection.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-package-update": [
            "@php artisan vendor:publish --provider='VanOns\\Laraberg\\LarabergServiceProvider' --tag='public' --force",
            "@php artisan vendor:publish --tag=lfm_config",
            "@php artisan vendor:publish --tag=lfm_public"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "symfony/thanks": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
